    "url": "https://github.com/haydenlinder/tickets/issues"
  },
  "homepage": "https://github.com/haydenlinder/tickets#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "mongo-seeding": "^3.4.0",
    "mongoose": "^5.9.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "superagent": "^5.2.2",
    "validator": "^12.2.0"
